{
    "SETTINGS": {
        "baseUrl": "https://test.k6.io/",
        "login": "https://test.k6.io/my_messages.php"
    },
    "UI": {
        "executor": "shared-iterations",
        "exec": "browserTest",
        "vus": 1,
        "duration": "3s",
